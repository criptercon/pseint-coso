Algoritmo Ej7
	Definir purchase, discount, total Como Real;
	Escribir "Ingrese el valor";
	Leer purchase;
	discount = purchase * 0.15;
	total = purchase - discount;
	Escribir "El valor incial es: " , purchase;
	Escribir "El descuento es: " , discount;
	Escribir "El valor total a pagar es: " , total;
FinAlgoritmo
