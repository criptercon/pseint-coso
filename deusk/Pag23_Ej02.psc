Algoritmo Pag23_Ej02
	
	Definir base, height, area, perimeter Como Entero;
	
	Escribir "Ingrese la base";
	Leer base;
	Escribir "Ingrese la altura";
	Leer height;
	area = base * height;
	perimeter =2 * height + 2 * base;
	
	Escribir  "El área del rectangulo es: " , area , ", y su perimetro es; " , perimeter;
FinAlgoritmo
