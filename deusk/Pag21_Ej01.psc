Algoritmo Pag21_Ej01
	Definir salary, minsalary como real;
	Escribir "Ingresa tu salario";
	Leer salary;
	Escribir "Indica el valor del SMLV";
	Leer minsalary;
	
	si salary > minsalary Entonces
		Escribir "Tu salario supera el mínimo";
	SiNo
		Escribir "Tu salario no supera el SMLV";
	FinSi
	
FinAlgoritmo
